{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build:cmake",
      "command": "cmake",
      "args": [
        "-DCMAKE_BUILD_TYPE=Debug",
        "../"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "label": "build:make",
      "command": "make",
      "args":[
        "-j"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "label": "build:debug",
      "dependsOrder": "sequence",
      "dependsOn": [
        "build:cmake",
        "build:make"
      ]
    }
  ]
}