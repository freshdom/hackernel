{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build:cmake:debug",
      "command": "cmake",
      "args": [
        "-DCMAKE_BUILD_TYPE=Debug",
        "../"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "label": "build:cmake:release",
      "command": "cmake",
      "args": [
        "-DCMAKE_BUILD_TYPE=Release",
        "../"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "label": "build:make",
      "command": "make",
      "args": [
        "-j"
      ],
      "options": {
        "cwd": "${workspaceFolder}/build/"
      }
    },
    {
      "label": "build:debug",
      "dependsOrder": "sequence",
      "dependsOn": [
        "build:cmake:debug",
        "build:make"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build:release",
      "dependsOrder": "sequence",
      "dependsOn": [
        "build:cmake:release",
        "build:make"
      ]
    }
  ]
}